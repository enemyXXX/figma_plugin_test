{
  "name": "figma-plugin-stage2-react-vkui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "watch:ui": "vite build --watch",
    "build:ui": "vite build",
    "watch:code": "esbuild src/code.ts --bundle --outfile=dist/plugin/code.js --format=iife --platform=browser --target=es2018 --watch",
    "build:code": "esbuild src/code.ts --bundle --outfile=dist/plugin/code.js --format=iife --platform=browser --target=es2018",
    "build": "npm run build:ui && npm run build:code",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "format": "prettier -w \"{src,**}/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@vkontakte/icons": "^2.105.0",
    "@vkontakte/vkui": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.117.0",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "esbuild": "^0.21.5",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.6.2",
    "typescript": "^5.4.0",
    "vite": "^5.0.0"
  }
}
